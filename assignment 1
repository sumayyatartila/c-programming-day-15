#include<stdio.h>
main()
{
	int n,i=1;
	scanf("%d",&n);
	while(i<=n)
	{
		if(i%2==0)
		printf("\n %d",i);
		i++;
	}


#include<stdio.h>
main()
{
  int i=1,even,odd,diff;
  even=odd=0;
  while(i<=20)
  {
  	if(i%2==0)
  	even=even+i;
  	else
  	odd=odd+i;
  	i++;
  }
  diff=even-odd;
  printf("diff:%d",diff);	
}

#include<stdio.h>
main()
{
	int i=1,n,fact=1;
	scanf("%d",&n);
	while(i<=n)
	{
		fact=fact*i;
		i++;
	}
	printf("%d",fact);
}
